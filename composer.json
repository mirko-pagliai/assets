{
    "name": "mirko-pagliai/assets",
    "description": "Assets plugin for CakePHP",
    "homepage": "https://github.com/mirko-pagliai/assets",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.2.0 <3.6",
        "matthiasmullie/minify": "^1.3"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "mirko-pagliai/reflection": "^1.0",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Assets\\Test\\": "tests",
            "TestPlugin\\": "tests/test_app/Plugin/TestPlugin/src"
        }
    }
}
